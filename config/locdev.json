{
  "server": {
    "port": 8011
  },
  "logging": {
    "override-global-console-methods": true,
    "file-logging": false,
    "file-transport-root-path": "./logs",
    "level": "silly",
    "skip-morgan-requests-log": true
  },
  "app": {
    "backup-path": ".backups",
    "apps-executable-path": ".executables",
    "apps-data-path": ".app-data",
    "artifacts-dir": ".code-builder/artifacts",
    "codebase-dir": ".code-builder/codebases",
    "nginx": {
      "dynamic-confs-dir": "/opt/homebrew/etc/nginx/code-deployer-routes.conf.d"
    }
  }
}
